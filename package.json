{
	"name": "@klenty/gunner",
	"version": "0.10.6",
	"description": "Zero magic, fast test-runner and assertion framework. No magic globals.",
	"main": "es6/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/klenty/gunner.git"
	},
	"keywords": [
		"klenty",
		"gunner",
		"test",
		"tdd",
		"unit testing"
	],
	"scripts": {
